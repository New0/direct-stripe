		"node-sass": "^4.13.1",
		"path": "^0.12.7",
		"postcss-loader": "^3.0.0",
		"postcss-preset-env": "^6.7.0",
		"sass-loader": "^8.0.2",
		"style-loader": "^1.1.3"
